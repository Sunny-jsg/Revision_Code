package com.sunny.in;

import java.util.Scanner;

public class FibonacciNumber {
	public static void main(String[] args) {
		int a = 0;
		int b = 1;
		int c;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter number");
		int num = sc.nextInt();
		for (int i = 1; i <= num; i++) {
			System.out.println(a);
			c = a + b;
			a = b;
			b = c;
		}
	}

}
